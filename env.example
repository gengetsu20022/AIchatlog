# ==========================================
# Firebase Configuration
# ==========================================
# ⚠️ 重要: このファイルはテンプレートです
# 実際の値は .env ファイルに設定してください
# .env ファイルは .gitignore で除外されています

# Firebase Web Configuration
FIREBASE_API_KEY=your_api_key_here
FIREBASE_APP_ID=your_app_id_here
FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id_here
FIREBASE_PROJECT_ID=your_project_id_here
FIREBASE_AUTH_DOMAIN=your_auth_domain_here
FIREBASE_STORAGE_BUCKET=your_storage_bucket_here
FIREBASE_MEASUREMENT_ID=your_measurement_id_here

# Firebase Android Configuration
FIREBASE_ANDROID_APP_ID=your_android_app_id_here

# Firebase iOS Configuration
FIREBASE_IOS_APP_ID=your_ios_app_id_here
FIREBASE_IOS_BUNDLE_ID=your_ios_bundle_id_here

# Firebase macOS Configuration
FIREBASE_MACOS_APP_ID=your_macos_app_id_here

# Firebase Windows Configuration
FIREBASE_WINDOWS_APP_ID=your_windows_app_id_here

# ==========================================
# Development Settings
# ==========================================
ENVIRONMENT=development
DEBUG_MODE=true

# ==========================================
# Security Settings
# ==========================================
# Rate limiting settings
MAX_REQUESTS_PER_MINUTE=60
MAX_REQUESTS_PER_HOUR=1000 